/**index.wxss**/

/* 最上面部分 */

.up {
  width: 100%;
  height: 100%;
}

.up-scan {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  z-index: 105;
  left: 30rpx;
}

.myswiper {
  width: 750rpx;
  height: 495rpx;
}

.slide-image {
  width: 750rpx;
  height: 495rpx;
}

.imgLoading{
  margin-top:136rpx;
}

.up-down {
  width: 100%;
  height: 300rpx;
  background: linear-gradient(to bottom, #1fce73, #cbf4dd);
}

/* 中间部分 */

.info {
  position: absolute;
  z-index: 88;
  width: 640rpx;
  border-radius: 15rpx;
  left: 25rpx;
  background: white;
  padding: 30rpx 30rpx 40rpx 30rpx;
  box-shadow: 2rpx 2rpx 13rpx 7rpx rgba(92, 92, 92, 0.1);
}

.info-up {
  display: flex;
  height: 80rpx;
}

.info-up-avatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
}

.info-up-mid, .info-up-name, .info-up-jihua {
  width: 450rpx;
}

.info-up-mid {
  height: 80rpx;
  margin-left: 20rpx;
}

.info-up-jihua {
  height: 60rpx;
  color: #818181;
  font-size: 25rpx;
}

.info-up-name {
  line-height: 30rpx;
  color: #818181;
  font-size: 25rpx;
}

.info-up-jihua {
  display: flex;
  line-height: 60rpx;
}

.info-up-jihua-icon {
  width: 26rpx;
  height: 26rpx;
  margin-top: 17rpx;
}

.info-up-jihua-text {
  margin-left: 10rpx;
}

.info-up-date {
  width: 90rpx;
  height: 90rpx;
  background-image: url('data:image/png;base64,');
  background-size: 90rpx;
  background-repeat: no-repeat;
  color: #66c2f8;
  text-align: center;
  line-height: 105rpx;
  font-size: 25rpx;
}

.info-down {
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
}

.info-down-item {
  display: flex;
}

.info-down-item-icon {
  width: 50rpx;
  height: 50rpx;
  margin-top: 20rpx;
}

.info-down-item-mid {
  height: 90rpx;
}

.info-down-item-mid, .info-down-item-num, .info-down-item-midtext {
  width: 100rpx;
}

.info-down-item-midtext {
  color: #818181;
  font-size: 22rpx;
  text-align: center;
}

.info-down-item-num {
  font-size: 50rpx;
  text-align: center;
  color: black;
}

.info-down-item-text {
  font-size: 22rpx;
  color: rgb(99, 99, 99);
}

/* 继续刷题 继续看课 */

.mid {
  width: 710rpx;
  display: flex;
  justify-content: space-around;
  padding: 0rpx 20rpx;
}

.mid-item {
  width: 330rpx;
  box-shadow: 1rpx 1rpx 13rpx 7rpx rgba(0, 0, 0, 0.1);
  border-radius: 15rpx;
  height: 230rpx;
}

.mid-item-image {
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 30rpx;
  background-size: 150rpx 50rpx;
  color: white;
  text-align: center;
  font-size: 28rpx;
}

.icon1 {
  background-image: url('data:image/png;base64,');
}

.icon2 {
  background-image: url('data:image/png;base64,');
}

.mid-item-title {
  font-size: 22rpx;
  text-align: center;
  width: 100%;
  margin-top: 20rpx;
  height: 75rpx;
}

.mid-item-bottom {
  width: 40rpx;
  height: 40rpx;
  margin-left: 275rpx;
}

/* 最下面的目录 */

.bottom {
  margin-top: 30rpx;
}

.bottom-line {
  display: flex;
  justify-content: space-around;
}

.bottom-line-item {
  width: 150rpx;
  padding: 20rpx 0rpx;
  border-radius: 15rpx;
}

.bottom-line-item-icon {
  width: 50rpx;
  height: 50rpx;
  margin-left: 50rpx;
}

.bottom-line-item-text {
  width: 150rpx;
  text-align: center;
  font-size: 25rpx;
}

.select1 {
  background: rgba(0, 0, 0, 0.1);
  border-radius: 10rpx;
}

.select2 {
  background: rgba(0, 0, 0, 0.1);
}

.select3{
  opacity: 0.3;
}
