/* components/jiaocheng/jiaocheng.wxss */

.container {
  position: fixed;
  bottom: 0rpx;
  z-index: 100;
}

.up {
  width: 750rpx;
  height: 98rpx;
  background-size: 750rpx 138rpx;
  background-repeat: no-repeat;
  color: white;
  text-align: center;
  font-size: 25rpx;
  padding: 0rpx 0rpx 40rpx 0rpx;
  background-image: url('data:image/jpeg;base64,');
}

/* 视频组件 */

.video {
  width: 750rpx;
  height: 422rpx;
}

.pause {
  width: 750rpx;
  height:422rpx;
  margin: 0 auto;
  z-index:103;
}

.video-icon{
  display: block;
  width:60rpx;
  height:60rpx;
  margin:0 auto;
  margin-top:130rpx;
  margin-bottom:50rpx;
}

.pause-text{
  width:100%;
  color:white;
  text-align: center;
  font-size:28rpx;
}

.prompt{
  width:100%;
  height:120rpx;
  margin-top:55rpx;
}

.prompt-top{
  font-size:22rpx;
  width:730rpx;
  line-height: 40rpx;
  padding-left:20rpx;
  text-align: center;
  background: white;
}

.hide{
  position: relative;
  font-size:30rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  letter-spacing: 8rpx;
  background: white;
  z-index:104rpx;
}

.model {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  filter: Alpha(Opacity=60);
  background-color: #1f1f1f;
  opacity: 0.5;
  z-index: 99;
}
