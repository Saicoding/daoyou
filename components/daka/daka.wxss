/* components/errorRecovery/errorRecovery.wxss */

.container {
  position: fixed;
  width: 450rpx;
  top: 250rpx;
  left: 150rpx;
  z-index: 110;
  border-radius: 10rpx;
}

.bar{
  position: relative;
  z-index:112;
  width:340rpx;
  height:80rpx;
  margin:0 auto;
  font-size:46rpx;
  color:white;
  text-align: center;
  background-size:340rpx 80rpx;
  background-image: url('data:image/png;base64,');
}

.content{
  position: relative;
  z-index:111;
  background:white;
  text-align: center;
  border-radius: 10rpx;
  top:-50rpx;
  padding-top:90rpx;
  padding-bottom:30rpx;
}

.content-text{
  font-size:35rpx;
  font-weight: bolder;
}

.content-icon{
  width:100rpx;
  height:100rpx;
  color:white;
  line-height: 100rpx;
  font-size:32rpx;
  margin:0 auto;
  margin-top:30rpx;
  background-size:100rpx 100rpx;
  background-image: url('data:image/jpeg;base64,');
}

.modal {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  filter: Alpha(Opacity=60);
  background: rgba(12, 12, 12, 0.3);
  z-index: 109;
}
