/* pages/shuati/tongji/tongji.wxss */
.all{
  position: absolute;
  width:100%;
  height:100%;
  top:0rpx;
  left:0rpx;
  background: white;
  z-index:103;
}

.container {
  width: 100%;
  padding-bottom:30rpx;
  background-repeat: no-repeat;
  background-size: 750rpx 570rpx;
  background-image: url('data:image/jpeg;base64,');
}

.back {
  position:fixed;
  width: 80rpx;
  height: 80rpx;
}

.back-icon {
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.title1,.title2{
  width:100%;
  text-align: center;
  color:white;
}

.title1{
  font-size:90rpx;
}

.title2{
  font-size:30rpx;
  letter-spacing: 5rpx;
}

.timeRate{
  width:80%;
  margin:0 auto;
  display: flex;
  justify-content: space-around;
  margin-top:20rpx;
}

.timeRate-item,.timeRate-item-text{
  width:200rpx;
}

.timeRate-item-text{
  height:60rpx;
  line-height: 60rpx;
  color:white;
  font-size:25rpx;
  text-align: center;
}

.jihuaZuoti{
  display: flex;
  justify-content: space-around;
  width:90%;
  margin:0 auto;
  margin-top:30rpx;
}

.xuexijihua,.jixuzuoti{
  width:220rpx;
  height:60rpx;
  line-height: 60rpx;
  font-size:30rpx;
  text-align: center;
  border-radius: 70rpx;
}

.xuexijihua{
  color:#31e59e;
  background: white;
}

.jixuzuoti{
  color:white;
  border:3rpx solid white;
}

/* 中间部分 */
.mid{
  display: flex;
  width:90%;
  margin:0 auto;
  justify-content:space-between;
  margin-top:40rpx;
}

/* 中间的线 */
.line{
  width:78%;
  margin:0 auto;
  margin-top:50rpx;
}

.line-1,.line-2{
  height:20rpx;
  border-radius: 20rpx;
}

.line-1{
  width:100%;
  background: #f2f2f2;
}

.line-2{
  position: relative;
  top:-20rpx;
  background: #15d25f;
}

/* 最下面 */
.down{
  width:85%;
  margin:0 auto;
  margin-top:30rpx;
}

.down-item{
  display: flex;
}

.down-item-icon{
  width:80rpx;
  height:80rpx;
  margin-top:5rpx;
}

.down-item-right{
  height:90rpx;
  font-size:25rpx;
}

.down-item-text{
  height:45rpx;
  margin-left:20rpx;
}

.select1{
  background: rgba(190, 190, 190,0.2);
}